#include <iostream>
#include <conio.h>

using namespace std;

int pgcd(int x, int y){

int x,y,tempo,resultat = 1, quotient = 1, reste = 1;

if (y>x) {
   tempo = y;
   y = x;
   x = tempo;
}
if (x % y == 0){
resultat = y;
}
else{
	while (reste!=0 and y!=0){
        quotient = x / y;
        reste = x % y;
        x = y;
        y = reste;
}
resultat = quotient
return resultat;
}

}
int main(void)
{   //début p.p
int a,b;
cout<< "Calcul PGCD, Saisir un 1er nb : ";
cin>>a;

cout<<"Saisir nb 2 : ";
cin>>b;

pgcd(a,b);

getch();
return 0;
}
